{
	"name": "resfes-frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
		"dev:frontend": "svelte-kit dev",
		"dev:backend": "cd backend && uvicorn main:app --reload --port 3001",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"start": "concurrently \"node build/index.js\" \"cd backend && gunicorn -k uvicorn.workers.UvicornWorker main:app --port 3001\"",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.55",
		"@sveltejs/kit": "next",
		"concurrently": "^6.4.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6"
	},
	"type": "module",
	"dependencies": {
		"axios": "^0.24.0",
		"font-awesome": "^4.7.0",
		"js-base64": "^3.7.2",
		"normalize.css": "^8.0.1"
	}
}
